ovieFilter';\nimport MovieCardDetail from './MovieCardDetail';\n\nfunction App() {\n  /*FETCH*/\n  useEffect(() => {\n    getApiData().then((dataClean) => {\n      setDataMovies(dataClean);\n    });\n  }, []);\n\n  //STATE VARIABLES//\n  /*API data*/\n  const [dataMovies, setDataMovies] = useState([]);\n\n  /*To filter by movie*/\n  const [movieFilter, setMovieFilter] = useState('');\n\n  /*State Variable to filter the years*/\n  const [yearFilter, setYearFilter] = useState([]);\n\n  //FUNCTIONS//\n  /*Function to modify the state variable of MovieFilter*/\n  const handleMovieFilter = (value) => {\n    setMovieFilter(value);\n  };\n\n  /*Function that has the value of years I want to filter */\n  const handleYearFilter = (value) => {\n    setYearFilter([...yearFilter, value]);\n  };\n\n  /*To take the filtered data*/\n  const userFilters = dataMovies\n    .filter((movie) => {\n      /*if (movieFilter === '') {\n      return true;\n    } else {\n      return movie.name === movieFilter;\n    }*/\n\n      //Ternary, it is the same as the previous conditional\n      return movieFilter === '' ? true : movie.title === movieFilter;\n    })\n    .filter((movie) => {\n      if (yearFilter.length === 0) {\n        return true;\n      } else {\n        return yearFilter.includes(movie.year);\n      }\n    });\n\n  /*Function to get the films years from the API, so I have to create a new array*/\n  const getYear = () => {\n    const movieYear = dataMovies.map((movie) => movie.year);\n    /*Function to avoid repeated years*/\n    const uniqueYear = movieYear.filter((year, index) => {\n      return movieYear.indexOf(year) === index;\n    });\n    return uniqueYear;\n  };\n\n  const { pathCard } = useLocation();\n  const dataPath = matchPath('/movie/:movieId', pathCard);\n\n  const movieId = dataPath.params.movieId;\n  const movieFound = dataMovies.find((movie) => movie.id === movieId);\n  //--------------------------------------------------//\n  return (\n    <>\n      <h1 className=\"header_title\">Owen Wilson's \"wow\" films</h1>\n      <Routes>\n        <Route\n          path=\"/\"\n          element={\n            <>\n              <Filters\n                handleMovieFilter={handleMovieFilter}\n                movieFilter={movieFilter}\n                getYear={getYear()}\n                handleYearFilter={handleYearFilter}\n              />\n              <MovieList movies={userFilters} />\n            </>\n          }\n        />\n        <Route\n          path=\"/movie/:movieId\"\n          element={<MovieCardDetail movieFound={movieFound} />}\n        />\n      </Routes>\n    </>\n  );\n}\n\nexport default App;\n","/Users/Rosa/Desktop/projects ADALAB/Módulo-3/EVALUACIONES/modulo-3-evaluacion-final-rosacodina/src/services/moviesApi.js",[],[],"/Users/Rosa/Desktop/projects ADALAB/Módulo-3/EVALUACIONES/modulo-3-evaluacion-final-rosacodina/src/components/MovieList.js",[],[],[],"/Users/Rosa/Desktop/projects ADALAB/Módulo-3/EVALUACIONES/modulo-3-evaluacion-final-rosacodina/src/components/CardMovie.js",["54","55"],[],"function CardMovie(props) {\n  return (\n    <a>\n      <section className=\"card\">\n        <div>\n          <img\n            className=\"card__image\"\n            alt={props.movie.image}\n            src={props.movie.image}\n          />\n        </div>\n\n        <div className=\"card__details\">\n          <h2>{`${props.movie.title} ${props.movie.year}`}</h2>\n          <p>{props.movie.phrase}</p>\n          <a>{props.movie.audio}</a>\n        </div>\n      </section>\n    </a>\n  );\n}\n\nexport default CardMovie;\n",[],"/Users/Rosa/Desktop/projects ADALAB/Módulo-3/EVALUACIONES/modulo-3-evaluacion-final-rosacodina/src/components/Filters.js",[],[],"/Users/Rosa/Desktop/projects ADALAB/Módulo-3/EVALUACIONES/modulo-3-evaluacion-final-rosacodina/src/components/MovieFilter.js",[],[],"/Users/Rosa/Desktop/projects ADALAB/Módulo-3/EVALUACIONES/modulo-3-evaluacion-final-rosacodina/src/components/YearFilter.js",[],[],[],"/